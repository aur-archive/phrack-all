prepare() {
    cd "$srcdir/"
    for i in $(seq $pkgver); do
        mkdir ${i}
        tar -C ${i} -xzvf ${_name}${i}.tar.gz > /dev/null
    done
}

package() {
    cd "$srcdir/"

    for i in $(seq $pkgver); do
        mkdir -p ${pkgdir}/usr/share/doc/${_name}/${i}
        install ${i}/* ${pkgdir}/usr/share/doc/${_name}/${i}
    done
}
